{
  "packageManager": "yarn@3.2.1",
  "name": "@artiefuzzz/arch",
  "version": "1.6.3",
  "description": "A random Discord API Wrapper",
  "main": "build/index.js",
  "typings": "build/types/index.d.ts",
  "author": "Artie Fuzzz",
  "license": "MIT",
  "files": [
    "build",
    "!build/*.tsbuildinfo"
  ],
  "scripts": {
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --cache --report-unused-disable-directives --ext .ts src/ --fix",
    "cleanup": "node scripts/cleanup.mjs",
    "build": "tsc -p src",
    "test": "ava",
    "postinstall": "husky install .github/.husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "discord-api-types": "*"
  },
  "dependencies": {
    "@artiefuzzz/lynx": "^2.4.1",
    "@sapphire/async-queue": "^1.3.1",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@artiefuzzz/eslint-config": "1.3.1",
    "@artiefuzzz/ts": "1.3.2",
    "@ava/typescript": "3.0.1",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@commitlint/cz-commitlint": "17.0.3",
    "@types/node": "17.0.45",
    "@types/ws": "8.5.3",
    "ava": "4.3.1",
    "commitizen": "4.2.4",
    "discord-api-types": "0.36.1",
    "eslint": "8.19.0",
    "husky": "8.0.1",
    "inquirer": "8.2.4",
    "lint-staged": "13.0.3",
    "pinst": "3.0.0",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "./src/**/*.ts": "yarn lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArtieFuzzz/arch.git"
  },
  "bugs": {
    "url": "https://github.com/ArtieFuzzz/arch/issues"
  },
  "keywords": [
    "discord",
    "wrapper",
    "api"
  ]
}
